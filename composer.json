{
    "name": "previewtechs/http-access-logger",
    "description": "HTTP access logs saved to any storage provider. ",
    "keywords": [
        "http-access",
        "log"
    ],
    "homepage": "http://github.com/PreviewTechnologies",
    "license": "MIT",
    "authors": [
        {
            "name": "Preview Technologies Limited",
            "email": "info@previewtechs.com",
            "homepage": "https://www.previewtechs.com/"
        }
    ],

    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "tomwalder/php-gds": "^3.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^2.0",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Previewtechs\\HTTP\\AccessLogger\\": "src",
            "Previewtechs\\HTTP\\AccessLogger\\Tests\\": "tests"
        }
    }
}
